    "description": "SDK for integrating AI agents with DeFi protocols",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "pnpm format && pnpm lint && tsup",
        "test": "vitest run",
        "lint": "eslint src --max-warnings 0",
        "prepare": "npm run build",
        "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
        "format:check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\"",
        "start:chatagent": "ts-node examples/cli-chat-agent/chat-agent.ts",
        "start:mcp-server": "ts-node examples/mcp-server/src/index.ts",
        "clean": "rimraf node_modules"
    },
    "keywords": [
        "defi",
        "ai",
        "agents",
        "blockchain",
        "ethereum",
        "sdk",
        "defi",
        "lending",
        "borrowing",
        "staking",
        "yield"
    ],
